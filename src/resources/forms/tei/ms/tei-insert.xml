<?xml version="1.0" encoding="UTF-8"?>

<!-- 
   This document is where you can add nodes to be inserted into the XML document you 
   are editing. The syntax I've used adds insert blocks that boilerplate xml is inserted into.
   This allows you to have inserts with default values or specific nodes present 
   
 -->
   

<instance >   
    
    <insert name="msItem">
        <msItem xmlns="http://www.tei-c.org/ns/1.0/" >
            <itemNo></itemNo>
            <defective/>
            <locus/>
            <author/>
            <title/>
            <rubric/>
            <incipit/>
            <explicit/>
            <decoNote/>
            <note/>
            <textLang></textLang>
        </msItem>
    </insert>
    
    <msBox xmlns="http://www.tei-c.org/ns/1.0/" ></msBox>
    <msFolder xmlns="http://www.tei-c.org/ns/1.0/" ></msFolder>
    <msItemNo xmlns="http://www.tei-c.org/ns/1.0/" ></msItemNo>

</instance>